<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NewsUpdate">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_home_screen_ek1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_240">


        <View
            android:id="@+id/_bg__home_screen_ek2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/_bg__home_screen_ek2_shape"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <View
                android:id="@+id/unsplash_mbqifklvowm_ek1"
                android:layout_width="376dp"
                android:layout_height="109dp"
                android:layout_marginStart="19dp"
                android:layout_marginTop="60dp"
                android:background="@drawable/radius"
                android:clickable="true"
                android:onClick="CalendarEvent" />

            <LinearLayout
                android:id="@+id/rectangle_10"
                android:layout_width="69dp"
                android:layout_height="71dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="79dp"
                android:background="@drawable/rectangle_10_shape"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="0dp"
                    android:gravity="center"
                    android:text="CSGO"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="0dp"
                    android:gravity="center"
                    android:text="18"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="AUGUST"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/rectangle_11"
                android:layout_width="69dp"
                android:layout_height="71dp"
                android:layout_marginStart="131dp"
                android:layout_marginTop="79dp"
                android:background="@drawable/rectangle_11_shape"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="COMPCLUB"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="22"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="JUNE"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/rectangle_12"
                android:layout_width="69dp"
                android:layout_height="71dp"
                android:layout_marginStart="212dp"
                android:layout_marginTop="79dp"
                android:background="@drawable/rectangle_12_shape"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="SOSPRO"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="15"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="APRIL"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/rectangle_13"
                android:layout_width="69dp"
                android:layout_height="71dp"
                android:layout_marginStart="292dp"
                android:layout_marginTop="79dp"
                android:background="@drawable/rectangle_13_shape"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="64dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="COMPSTUD"
                    android:textSize="11.5sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="16"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="FEBRUARY"
                    android:textSize="11sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/idRVNews"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/_bg__home_screen_ek2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cardView2"
            app:layout_constraintVertical_bias="0.0">

        </androidx.recyclerview.widget.RecyclerView>

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.cardview.widget.CardView
        android:id="@+id/frame_240"
        android:layout_width="0dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <View
            android:id="@+id/_bg__frame_240_ek1"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"  />

        <RelativeLayout
            android:id="@+id/menu_profil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="6dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true">

            <View
                android:id="@+id/_bg__menu_profil_ek1"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/_bg__menu_profil_ek1_shape" />

            <ImageView
                android:id="@+id/ellipse_251"
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:layout_marginStart="3.5dp"
                android:layout_marginTop="3.5dp"
                android:contentDescription="ellipse_251"
                android:src="@drawable/ellipse_251" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/notification_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="355dp"
            android:layout_marginTop="6dp">

            <View
                android:id="@+id/_bg__notification_button_ek1"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/_bg__notification_button_ek1_shape" />

            <ImageView
                android:id="@+id/combo_shape"
                android:layout_width="17dp"
                android:layout_height="20dp"
                android:layout_marginStart="13.5dp"
                android:layout_marginTop="12dp"
                android:contentDescription="combo_shape"
                android:src="@drawable/combo_shape" />

        </RelativeLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
