<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/NewsRVItem"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_margin="5dp"
	app:cardCornerRadius="6dp"
	app:cardElevation="4dp">

		<View
			android:id="@+id/rectangle_8"
			android:layout_width="match_parent"
			android:layout_height="338dp"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp"
			android:background="@drawable/rectangle_8_shape" />


		<RelativeLayout
			android:id="@+id/frame_243"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="0dp"
			android:layout_marginTop="50dp">

			<View
				android:id="@+id/_bg__frame_243_ek1"
				android:layout_width="370dp"
				android:layout_height="288dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp" />

			<ImageView
				android:id="@+id/ImageNewsFill"
				android:layout_width="350dp"
				android:layout_height="268dp"
				android:layout_marginStart="15dp"
				android:layout_marginTop="10dp"
				android:contentDescription="There Is No Description"
				android:src="@drawable/unsplash_mbqifklvowm" />

		</RelativeLayout>


		<TextView
			android:id="@+id/newsTitleFill"
			android:layout_width="186dp"
			android:layout_height="32dp"
			android:layout_marginStart="71dp"
			android:layout_marginTop="20dp"
			android:layout_marginEnd="119dp"
			android:text="PUMA COMPUTING 2023"
			android:ellipsize="end"
			android:maxLines="2"
			android:textStyle="bold" />

		<RelativeLayout
			android:id="@+id/frame_2421"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="15dp"
			android:layout_marginTop="281dp">


			<View
				android:id="@+id/_bg__frame_242_ek12"
				android:layout_width="350dp"
				android:layout_height="47dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:background="@drawable/radius2"
				android:foregroundGravity="bottom"
				android:visibility="visible"
				tools:ignore="NotSibling" />

			<TextView
				android:id="@+id/idArticleFill"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:ellipsize="end"
				android:maxLines="2"
				android:text="Article" />


		</RelativeLayout>


		<RelativeLayout
			android:id="@+id/avatar"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="20dp"
			android:layout_marginTop="10dp">


			<RelativeLayout
				android:id="@+id/avatar_ek2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp">


				<View
					android:id="@+id/ellipse_250"
					android:layout_width="40dp"
					android:layout_height="40dp"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:background="@drawable/ellipse_250_shape" />

				<ImageView
					android:id="@+id/unsplash__h6wpor9mjs"
					android:layout_width="36dp"
					android:layout_height="36dp"
					android:layout_marginStart="2dp"
					android:layout_marginTop="2dp"
					android:contentDescription="unsplash__h6wpor9mjs"
					android:src="@drawable/unsplash__h6wpor9mjs" />

			</RelativeLayout>

		</RelativeLayout>
</androidx.cardview.widget.CardView>
